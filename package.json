{
  "name": "tobie-command-center",
  "version": "0.1.0",
  "private": true,
  "description": "Tobie Command Center - Web Portal + Desktop Agent",
  "author": "Tobie Team <josh@tobie.team>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rimraf node_modules",
    "db:push": "pnpm --filter web db:push",
    "db:seed": "pnpm --filter web db:seed",
    "db:studio": "pnpm --filter web db:studio",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "turbo": "^1.12.0",
    "typescript": "^5.3.0"
  }
}
